{
  "name": "e-commerce-case",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "build:staging": "tsc && yarn vite build --mode staging",
    "preview": "vite preview",
    "prepare": "husky install",
    "prestart": "yarn build",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --max-warnings=0 --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc",
    "type-check:no-artifact": "tsc --skipLibCheck --noEmit",
    "release": "standard-version"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.18",
    "@chakra-ui/react": "^2.5.5",
    "@chakra-ui/theme-tools": "^2.0.5",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@splidejs/react-splide": "^0.7.12",
    "classnames": "^2.3.2",
    "framer-motion": "^10.12.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.10.0",
    "swiper": "^9.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "happy-dom": "^9.1.7",
    "husky": "^8.0.1",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.7",
    "react-test-renderer": "^18.2.0",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.0.4",
    "vitest": "^0.29.8"
  }
}
